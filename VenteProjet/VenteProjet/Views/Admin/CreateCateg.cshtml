@model VenteProjet.Models.Categorie

@{
    ViewBag.Title = "CreateCateg";
}


@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Categorie</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.RefCat)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.RefCat)
            @Html.ValidationMessageFor(model => model.RefCat)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Nomcatg)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Nomcatg)
            @Html.ValidationMessageFor(model => model.Nomcatg)
        </div>

        <p>
            <input type="submit" value="Nouveau" />
        </p>
    </fieldset>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
